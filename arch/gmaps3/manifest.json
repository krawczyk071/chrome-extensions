{
  "manifest_version": 2,
  "name": "MapPhotoSaver",
  "version": "1.0",
  "permissions": ["activeTab", "storage", "https://*.google.com/*"],
  "content_scripts": [
    {
      "matches": ["https://www.google.com/maps/*"],
      "js": ["content.js"]
    }
  ],
  "browser_action": {
    "default_popup": "popup.html"
  }
}
